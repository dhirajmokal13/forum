{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node",
            "config": {
            "includeFiles": ["views/**","public/**"]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "./"
        }
    ]
}